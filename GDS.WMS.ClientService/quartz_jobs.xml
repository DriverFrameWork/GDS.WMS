<?xml version="1.0" encoding="UTF-8"?>

<!-- This file contains job definitions in schema version 2.0 format -->

<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">

  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>
    <!--定义同步物料基本信息任务 每一分钟同步一次-->
    <job>
      <name>ACIOJob</name>
      <group>ACIOGroup</group>
      <description>同步物料基本信息任务</description>
      <job-type>GDS.WMS.ClientService.ACIOService,GDS.WMS.ClientService</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <!--定义示例任务1 触发器 每10分钟执行一次DemoJob1任务-->
    <trigger>
      <cron>
        <name>ACIOTrigger</name>
        <group>ACIOTriggerGroup</group>
        <job-name>ACIOJob</job-name>
        <job-group>ACIOGroup</job-group>
        <cron-expression>0 0/5 * * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
</job-scheduling-data>